# g4 nightmare
## Perforce
export P4CONFIG=.p4config
export P4EDITOR=$EDITOR
cl_num() {
  p4 -F'%change%' changes -s pending -c "$(p4 -F'%clientName%' info)"
}

## Beyond Compare
export G4MULTIDIFF=1 # All diffs simultaneously
export P4DIFF=$HOME/bin/p4bcdiff.sh
export P4MERGE=$HOME/bin/p4bcmerge.sh

## presubmit
g4p() {
  clientdir="$(g4 -ztag -F"%client%" describe -c $1)"
  nohup g4 -c ${clientdir} presubmit --ignore_presubmit_errors >> /dev/null 2>&1&
}


